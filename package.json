{
  "name": "find-route",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "npx prettier . --write",
    "bump-version": "node scripts/generate-version.js",
    "build": "npm run bump-version && vue-tsc -b && vite build",
